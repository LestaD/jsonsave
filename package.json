{
  "name": "jsonsave",
  "version": "3.0.1",
  "description": "Simple open, edit and save .json files",
  "main": "bin/jsonsave.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha -r should -R spec",
    "clean-cov": "rm -rf coverage/ coverage.html",
    "prepare-cov": "./node_modules/.bin/jscoverage --encoding=utf8 --no-highlight bin coverage",
    "coverage": "npm run prepare-cov && COVERAGE=1 ./node_modules/.bin/mocha -r should -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LestaD/jsonsave.git"
  },
  "keywords": [
    "json",
    "file",
    "save"
  ],
  "author": "LestaD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LestaD/jsonsave/issues"
  },
  "homepage": "https://github.com/LestaD/jsonsave#readme",
  "devDependencies": {
    "conlog": "0.0.9",
    "mocha": "2.3.3",
    "should": "6.0.3",
    "supertest": "1.1.0"
  }
}
